@model Turnos.Models.TransporteTipo

@{
    ViewData["Title"] = "Details";
}

<div>
    <h1>Tipo de Transporte</h1>
    <hr />
    <div class="table-responsive-sm">
        <table>
            <tbody>
                <tr>
                    <td>
                        Nombre
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.Nombre)
                    </td>
                </tr>
                <tr>
                    <td>
                        Descripcion
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.Descripcion)
                    </td>
                </tr>
                <tr>
                    <td>
                        Activo
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.Activo)
                    </td>
                </tr>
                
            </tbody>
        </table>
    </div>
    
<div>
    <a asp-action="Edit" asp-route-id="@Model.IdTransporteTipo">Editar</a>    
</div>
